{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "GEMINI_MODEL": "gemini-1.5-flash",
    "TRANSLATION_MAX_TOKENS": "4000",
    "TRANSLATION_TEMPERATURE": "0.3"
  }
}
